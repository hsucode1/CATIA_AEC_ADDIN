  <Template syp:name="AECDraftigAnnotationCommandHeaders" Target="CATAfrComponent">
    <CATCAfrGroup>

      <CATCommandHeader  ID="CATAECAnnAngleThreePointHdr" ClassName="CATAECAnnAngleThreePointCmd" SharedLibraryName="CATAEC3DAnnotationUI" ResourceFile="CATAECAnnotationAddinHeader"  Available="1"/>
      <CATCommandHeader  ID="CATAECAnnOnePointHdr" ClassName="CATAECAnnOnePointCmd" SharedLibraryName="CATAEC3DAnnotationUI" ResourceFile="CATAECAnnotationAddinHeader"  Available="1"/>
      <CATCommandHeader  ID="CATAECAnnDimTwoPointHdr" ClassName="CATAECAnnDimTwoPointCmd" SharedLibraryName="CATAEC3DAnnotationUI" ResourceFile="CATAECAnnotationAddinHeader"  Available="1"/>
      <CATCommandHeader  ID="CATAECAnnAngleTwoLineHdr" ClassName="CATAECAnnAngleTwoLineCmd" SharedLibraryName="CATAEC3DAnnotationUI" ResourceFile="CATAECAnnotationAddinHeader"  Available="1"/>
      <CATCommandHeader  ID="CATAECAnnRadiusHdr" ClassName="CATAECAnnRadiusCmd" SharedLibraryName="CATAEC3DAnnotationUI" ResourceFile="CATAECAnnotationAddinHeader"  Available="1"/>
    
      
      <!--Drawing-->

      <CATCommandHeader  ID="CATDRWPositionHdr" ClassName="CATDRWPositionCmd" SharedLibraryName="CATDRWPositionFeatureUI" ResourceFile="CATAECAnnotationAddinHeader"  Available="1"/>

      <!-- <CATCommandHeader  ID="CATExtractBoundaryHdr" ClassName="CATExtractBoundaryCmd" SharedLibraryName="CATExtractBoundary" ResourceFile="AECDraftingWkbAddinHeader" Available="1"/> -->

    </CATCAfrGroup>
  </Template>

