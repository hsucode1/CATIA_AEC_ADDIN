      <CATCmdStarter Name="CATAECAnnRadiusHdr" Command="CATAECAnnRadiusHdr"/>
      <CATCmdStarter Name="CATAECAnnAngleTwoLineHdr" Command="CATAECAnnAngleTwoLineHdr"/>
      <!-- <CATCmdStarter Name="CATAECAnnAngleThreePointHdr" Command="CATAECAnnAngleThreePointHdr"/> -->
    </CATCmdContainer>
  </Template>

  <!-- Define a Section -->
